# Test Environment Variables
NODE_ENV=test

# API Configuration (Mock Server)
VITE_API_URL=http://localhost:3001/api/v1
VITE_WS_URL=ws://localhost:3001/ws

# Application
VITE_APP_NAME=QA Learning App Test
VITE_APP_VERSION=1.0.0-test
VITE_APP_DESCRIPTION=Test environment

# Features Flags (All enabled for testing)
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_REALTIME_CHAT=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_TWO_FACTOR_AUTH=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_EMAIL_VERIFICATION=true
VITE_ENABLE_PAYMENT=true

# Authentication
VITE_AUTH_TOKEN_KEY=test_accessToken
VITE_AUTH_REFRESH_TOKEN_KEY=test_refreshToken
VITE_SESSION_TIMEOUT=60000

# File Upload
VITE_MAX_FILE_SIZE=1048576
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png

# Test Configuration
VITE_TEST_USER_EMAIL=test@example.com
VITE_TEST_USER_PASSWORD=TestPass123!
VITE_TEST_API_KEY=test-api-key-12345

# Development Tools
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_LOGGER=true
VITE_LOG_LEVEL=info