# Production Environment Variables

# API Configuration
VITE_API_URL=https://api.qa-learning.com/api/v1
VITE_WS_URL=wss://api.qa-learning.com/ws

# Application
VITE_APP_NAME=QA Learning App
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Interactive learning platform for QA professionals

# Features Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_REALTIME_CHAT=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_TWO_FACTOR_AUTH=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_EMAIL_VERIFICATION=true
VITE_ENABLE_PAYMENT=true

# Authentication
VITE_AUTH_TOKEN_KEY=accessToken
VITE_AUTH_REFRESH_TOKEN_KEY=refreshToken
VITE_SESSION_TIMEOUT=1800000

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf

# Analytics
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
VITE_SENTRY_DSN=https://xxx@sentry.io/xxx

# Social Login
VITE_GOOGLE_CLIENT_ID=xxx.apps.googleusercontent.com
VITE_GITHUB_CLIENT_ID=xxx
VITE_FACEBOOK_APP_ID=xxx

# Production Settings
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_LOGGER=false
VITE_LOG_LEVEL=error